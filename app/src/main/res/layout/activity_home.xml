<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#24293E"
    tools:context=".MainActivity"
    android:padding="20dp"
    android:backgroundTint="@drawable/vector_2.xml"
    android:fillColor="#24293E">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/table_home"
        android:layout_width="wrap_content"
        android:layout_height="165dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="89dp"
        android:background="#00000000">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/heart_home"
            android:layout_width="39dp"
            android:layout_height="39dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="#00000000"
            tools:layout_editor_absoluteX="300dp"
            tools:layout_editor_absoluteY="107dp">

            <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/component_9"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="308dp"
                android:layout_marginTop="204dp"
                android:background="@drawable/component_9"
                android:clipToOutline="true"></RelativeLayout>

            <View
                android:id="@+id/vector"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:background="@drawable/vector" />

            <View
                android:id="@+id/ellipse_5"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="304dp"
                android:layout_marginTop="200dp"
                android:background="@drawable/ellipse_5" />

            <View
                android:id="@+id/ellipse_4"
                android:layout_width="39dp"
                android:layout_height="39dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="300dp"
                android:layout_marginTop="196dp"
                android:background="@drawable/ellipse_4" />

        </androidx.appcompat.widget.LinearLayoutCompat>

        <ImageView
            android:id="@+id/img_table"
            android:layout_width="370dp"
            android:layout_height="165dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/img_table"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="0dp">

        </ImageView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/content_tab"
            android:layout_width="102dp"
            android:layout_height="39dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="#00000000"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="110dp">

            <View
                android:id="@+id/rectangle_2"
                android:layout_width="102dp"
                android:layout_height="39dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="36dp"
                android:layout_marginTop="196dp"
                android:background="@drawable/rectangle_2" />

            <TextView
                android:id="@+id/kh_m_ph_"
                android:layout_width="78dp"
                android:layout_height="18dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:gravity="center_vertical"
                android:lineSpacingExtra="4sp"
                android:text="@string/kham_pha"
                android:textAppearance="@style/kham_pha"
                app:layout_constraintEnd_toEndOf="@+id/rectangle_2"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="13dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/konnichiwa"
            android:layout_width="125dp"
            android:layout_height="35dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical"
            android:lineSpacingExtra="7sp"
            android:text="@string/konnichiwa"
            android:textAppearance="@style/konnichiwa"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="38dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/user_home"
        android:layout_width="343dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:background="#00000000"
        app:layout_anchor="@+id/user_home"
        app:layout_anchorGravity="center">

        <View
            android:id="@+id/avatar_home"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/avatar_home"
            tools:layout_editor_absoluteX="291dp"
            tools:layout_editor_absoluteY="7dp"></View>

        <TextView
            android:id="@+id/name_user_h"
            android:layout_width="138dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical"
            android:text="@string/name_user_h"
            android:textAppearance="@style/name_user_h"
            app:layout_anchor="@+id/user_home"
            app:layout_anchorGravity="start|center"
            tools:layout_editor_absoluteX="3dp"
            tools:layout_editor_absoluteY="22dp"></TextView>

        <TextView
            android:id="@+id/welcome_use"
            android:layout_width="138dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical"
            android:text="@string/welcome_use"
            android:textAppearance="@style/welcome_use"
            tools:layout_editor_absoluteX="3dp"
            tools:layout_editor_absoluteY="2dp"></TextView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>
